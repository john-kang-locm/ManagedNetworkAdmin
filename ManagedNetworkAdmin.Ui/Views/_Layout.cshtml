<!DOCTYPE html>
<html lang="en" ng-app="app">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title ng-bind="title">@(PageData["title"] ?? "AngularJS SPA Template for Visual Studio")</title>
    <meta name="description" content='@PageData["description"]'>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    @Styles.Render("~/content/css/app")
</head>
<body>
    <header class="navbar navbar-default" role="banner" ng-if=" !layout ">

        <div class="container">
            <div class="navbar-left">
                <h3>Managed Network Admin</h3>
            </div>
            <div class="navbar-right" style="vertical-align: middle; margin-top: 20px">
                <div ng-controller="SearchCtrl">
                    <form class="form-search">
                        <input type="text" ng-model="keywords" ng-click="search()" class="input-medium search-query" placeholder="Search..."><img src="~/Images/searchButton.png" /></input>
                    </form>
                </div>
            </div>
        </div>
